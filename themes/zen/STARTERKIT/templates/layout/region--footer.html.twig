{% extends "@STARTERKIT/components/footer/footer.twig" %}
{#
/**
 * @file
 * Theme override to display the footer region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}

{# Move attributes' class into the modifier_class variable. #}
{% set modifier_class = attributes.storage().class|join(' ') %}
{% set attributes = attributes.removeAttribute('class') %}
